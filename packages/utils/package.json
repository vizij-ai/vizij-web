{
  "name": "@semio/utils",
  "description": "Shared typescript utilities.",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts src/index.ts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts src/index.ts --external react",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@semio/eslint-config": "*",
    "@semio/typescript-config": "*",
    "@types/color": "^3.0.6",
    "@types/deep-equal": "^1.0.4",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.170.0",
    "eslint": "^8.57.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "color": "^4.2.3",
    "deep-equal": "^2.2.3",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "three": "^0.170.0",
    "fast-deep-equal": "^3.1.3",
    "zustand": "^5.0.2"
  }
}
